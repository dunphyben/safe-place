light-gray = #F5F5F5

// Quick ul css reset
ul
ol
  list-style none
  padding 0
  margin 0

#menu-bar
  border-top 1px solid txt-gray
  max-height 100px
  background light-gray
  color white
  position absolute
  bottom 0
  left 0
  right 0

  a
    display block
    width 100%
    padding 10px 0
    span
      line-height 1
      font-weight 300
      font-size 12px



